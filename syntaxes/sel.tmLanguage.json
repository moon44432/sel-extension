{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "SEL",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#comments"
		},
		{
			"include": "#numerics"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.sel",
				"match": "\\b(if|then|else|while|for|rept|loop|return|break)\\b"
			},
			{
				"name": "keyword.declare.sel",
				"match": "\\b(arr|func)\\b"
			}]
		},
		"comments": {
			"name": "comment.sel",
			"begin": "#",
			"patterns": [{
				"match": "[^\n\r]*"
			}]
		},
		"numerics": {
			"patterns": [{
				"name": "constant.numeric.literal.sel",
				"match": "[+-]?([0-9]*[.])?[0-9]+"
			}]
		},
		"strings": {
			"name": "string.quoted.double.sel",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.sel",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.sel"
}